<Window x:Class="wpfcrud.deicde"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wpfcrud"
        mc:Ignorable="d"
        Title="deicde" Height="450" Width="800">
    <Grid>
        <Button x:Name="upd" Content="Update" HorizontalAlignment="Left" Margin="161,217,0,0" VerticalAlignment="Top" Height="60" Width="148" Click="updateUser"/>
        <Button x:Name="del" Content="Delete" HorizontalAlignment="Left" Margin="440,217,0,0" VerticalAlignment="Top" Height="60" Width="148" Click="deleteUser"/>
        <TextBlock x:Name="id" HorizontalAlignment="Left" Margin="235,91,0,0" TextWrapping="Wrap" Text="WTF Do You Want?" VerticalAlignment="Top"/>
        <TextBlock x:Name="username" HorizontalAlignment="Left" Margin="175,138,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Visibility="Hidden"/>
        
        <TextBlock x:Name="password" HorizontalAlignment="Left" Margin="468,138,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" Visibility="Hidden"/>

    </Grid>
</Window>
